1-Weird Algorithm
public static void main(String[] args) {
		FastReader in= new FastReader();
		long t= in.nextInt();
		System.out.println(t+" ");
		long x;
		while(t!=1) {
			if(t%2==0) {
				 x =even(t);
				System.out.println(x);
				
			}
			else {
				 x=odd(t);
				System.out.println(x);
			
			}
			t=x;
		}
		
	}
	public static long even(long x) {
		return x/2;
	}
	public static long odd (long x) {
		return (x*3)+1;
	}
	
  
  
  
