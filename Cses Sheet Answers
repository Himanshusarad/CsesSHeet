10---> Ferris Wheel


public static void main(String[] args) {
		FastReader in= new FastReader();
		int n = in.nextInt();
		int x = in.nextInt();
		
		int a[] = in.readintarray(n);
		Arrays.sort(a);
		int count=0;
		for(int i=0,j=n-1;i<j;) {
			while(a[i]+a[j]>x&&i<j) {
				j--;
//				System.out.println(1);
			}
//			System.out.println(i+" "+j);
			if(i>=j) { 
//				System.out.println(1);
				break;
			}
			count++;
			i++;
			j--;
				
			
		}
		System.out.println(n-count);
		
		
	}
	
	
	
