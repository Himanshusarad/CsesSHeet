8---> Distinct Numbers



	public static void main(String[] args) {
		FastReader in= new FastReader();
		int n = in.nextInt();
		int a[] = new int[n];
		HashSet<Integer> h = new HashSet<>();
		for(int i =0;i<n;i++) {
			a[i]=in.nextInt();
			h.add(a[i]);
		}
		System.out.println(h.size());
	}
	
	
	
	
