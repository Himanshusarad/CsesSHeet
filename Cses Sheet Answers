  public static void main(String[] args) {
		FastReader in= new FastReader();
		int n = in.nextInt();
		int a[] = new int [n-1];
		long sum=0;
		for(int i =0;i<n-1;i++) {
			a[i]=in.nextInt();
			sum+=a[i];		
		}
		long y = n*(n+1)/2;
		System.out.println(y-sum);
	}
  
  
  
